{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "online.html",
      "use": "@vercel/static"
    },
    {
      "src": "polling-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/polling-server.js"
    },
    {
      "src": "/online",
      "dest": "/online.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}