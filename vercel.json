{
  "version": 2,
  "builds": [
    {
      "src": "polling-server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "guangzhou-metro-new.js",
          "shenzhen-metro.js",
          "shanghai-metro.js",
          "metro-data-manager.js",
          "game.js",
          "style.css",
          "index.html",
          "online.html",
          "online-game.js",
          "polling-game.js",
          "validate-stations.js"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/polling-server.js"
    }
  ]
}